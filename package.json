{
  "name": "Hera Widget",
  "description": "A React-based widget for Hera, providing a customizable chat interface.",
  "author": "HERA",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "finalize:release": "shx mkdir -p dist && shx cp build/static/js/main.js ./dist/widget.min.js",
    "finalize": "shx mkdir -p dist && shx cp build/static/js/main.js ./dist/widget.js",
    "build:release": "REACT_APP_ENV=production react-app-rewired build && yarn finalize:release",
    "build": "REACT_APP_ENV=staging react-app-rewired build && yarn finalize",
    "build:release-win": "cross-env REACT_APP_ENV=production react-app-rewired build && yarn finalize:release",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@tailwindcss/forms": "^0.5.3",
    "@tanstack/react-table": "^8.20.5",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "dayjs": "^1.11.8",
    "immer": "^10.0.2",
    "linkify-react": "^4.1.1",
    "linkifyjs": "^4.1.1",
    "motion": "^12.18.1",
    "nanoid": "^4.0.2",
    "rc-scrollbars": "^1.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.5",
    "react-hook-form": "^7.44.3",
    "react-markdown": "^9.0.1",
    "react-query": "^3.39.3",
    "react-scripts": "5.0.1",
    "react-spinners": "^0.13.8",
    "rehype-parse": "^9.0.1",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "store2": "^2.14.4",
    "styled-components": "^6.1.14",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "react-app-rewired": "^2.2.1",
    "shx": "^0.3.4",
    "tailwindcss": "^3.3.2",
    "terser-webpack-plugin": "^5.3.9",
    "uglifyjs-webpack-plugin": "^2.2.0"
  }
}
